generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users{
  id String @id @default(uuid())
  email    String  @unique
  password String
  name     String
  expense  expense[]
}

model expense{
  id String @id @default(uuid())
  amount BigInt 
  date DateTime 
  month String
  label String
  userid String
  users users @relation(fields: [userid], references: [id])
}